      "type": "Tilt",
      "fineChannelAliases": ["Tilt fine"],
      "defaultValue": 32768
    },
    "Pan / Tilt Speed": {
      "type": "Speed",
      "invert": true,
      "capabilities": [
        {
          "range": [0, 255],
          "name": "Decreasing speed"
        }
      ]
    },
    "Red": {
      "type": "Single Color",
      "color": "Red"
    },
    "Green": {
      "type": "Single Color",
      "color": "Green"
    },
    "Blue": {
      "type": "Single Color",
      "color": "Blue"
