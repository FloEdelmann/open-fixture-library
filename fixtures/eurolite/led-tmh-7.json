    "Color Macros": {
      "type": "Multi-Color",
      "capabilities": [
        {
          "range": [0, 40],
          "name": "Neutral"
        },
        {
          "range": [41, 80],
          "name": "Macro 1"
        },
        {
          "range": [81, 120],
          "name": "Macro 2"
        },
        {
          "range": [121, 140],
          "name": "Macro 3"
        },
        {
          "range": [141, 160],
          "name": "Macro 4"
        },
        {
          "range": [161, 200],
          "name": "Macro 5"
        },
        {
          "range": [201, 240],
          "name": "Macro 6"
        },
        {
          "range": [241, 255],
          "name": "Neutral"
        }
      ]
    },
    "Shutter": {
      "type": "Shutter",
      "defaultValue": 255,
      "capabilities": [
        {
          "range": [0, 10],
          "name": "LED off",
          "menuClick": "start"
        },
        {
          "range": [11, 20],
          "name": "Neutral",
          "menuClick": "hidden"
        },
        {
          "range": [21, 30],
          "name": "Reset",
          "menuClick": "start"
        },
        {
          "range": [31, 200],
          "name": "Strobe slow - fast",
          "menuClick": "center"
        },
        {
          "range": [201, 250],
          "name": "Random strobe slow - fast",
          "menuClick": "center"
        },
        {
          "range": [251, 255],
          "name": "Neutral",
          "menuClick": "start"
        }
      ]
    },
