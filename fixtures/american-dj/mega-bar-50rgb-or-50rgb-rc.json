{
  "$schema": "https://raw.githubusercontent.com/OpenLightingProject/open-fixture-library/master/schemas/fixture.json",
  "name": "Mega Bar 50RGB or 50RGB RC",
  "categories": ["Color Changer", "Strobe", "Effect"],
  "meta": {
    "authors": ["Peter G."],
    "createDate": "2019-11-10",
    "lastModifyDate": "2019-11-10"
  },
  "links": {
    "manual": [
      "https://cdb.s3-us-west-1.amazonaws.com/ItemRelatedFiles/7103/mega_bar_50rgb_rc_rev_6_11.pdf"
    ],
    "productPage": [
      "https://www.adj.com/mega-bar-50rgb-rc"
    ],
    "video": [
      "https://www.youtube.com/watch?v=8Px1FM_7BU8"
    ]
  },
  "physical": {
    "dimensions": [560, 63, 102],
    "weight": 1.5,
    "power": 18,
    "DMXconnector": "3-pin",
    "bulb": {
      "type": "125× 10mm LEDs (24× red, 54× green, 47× blue)"
    },
    "lens": {
      "degreesMinMax": [40, 40]
    }
  },
  "availableChannels": {
    "Color Macros": {
      "capability": {
        "type": "ColorPreset"
      }
    },
    "Intensity": {
      "capability": {
        "type": "Intensity"
      }
    },
    "Red": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Red"
      }
    },
    "Green": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Green"
      }
    },
    "Blue": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Blue"
      }
    },
    "Strobe": {
      "capabilities": [
        {
          "dmxRange": [0, 15],
          "type": "NoFunction"
        },
        {
          "dmxRange": [16, 255],
          "type": "ShutterStrobe",
          "shutterEffect": "Strobe",
          "speedStart": "slow",
          "speedEnd": "fast"
        }
      ]
    },
    "Program Speed": {
      "capability": {
        "type": "EffectSpeed",
        "speedStart": "slow",
        "speedEnd": "fast"
      }
    },
    "Sound Sensitivity": {
      "capability": {
        "type": "SoundSensitivity",
        "soundSensitivityStart": "low",
        "soundSensitivityEnd": "high"
      }
    },
    "Program": {
      "defaultValue": 0,
      "capabilities": [
        {
          "dmxRange": [0, 127],
          "type": "NoFunction",
          "switchChannels": {
            "Strobe / Program Speed / Sound Sensitivity": "Strobe"
          }
        },
        {
          "dmxRange": [128, 159],
          "type": "Effect",
          "effectPreset": "ColorFade",
          "comment": "Color Mixing",
          "switchChannels": {
            "Strobe / Program Speed / Sound Sensitivity": "Program Speed"
          }
        },
        {
          "dmxRange": [160, 191],
          "type": "Effect",
          "effectPreset": "ColorJump",
          "comment": "3 Color Change",
          "switchChannels": {
            "Strobe / Program Speed / Sound Sensitivity": "Program Speed"
          }
        },
        {
          "dmxRange": [192, 223],
          "type": "Effect",
          "effectPreset": "ColorJump",
          "comment": "7 Color Change",
          "switchChannels": {
            "Strobe / Program Speed / Sound Sensitivity": "Program Speed"
          }
        },
        {
          "dmxRange": [224, 255],
          "type": "Effect",
          "effectName": "Sound Active",
          "soundControlled": true,
          "switchChannels": {
            "Strobe / Program Speed / Sound Sensitivity": "Sound Sensitivity"
          }
        }
      ]
    }
  },
  "modes": [
    {
      "name": "1-channel",
      "shortName": "1ch",
      "channels": [
        "Color Macros"
      ]
    },
    {
      "name": "w-channel",
      "shortName": "wch",
      "channels": [
        "Color Macros",
        "Intensity"
      ]
    },
    {
      "name": "3-channel",
      "shortName": "3ch",
      "channels": [
        "Red",
        "Green",
        "Blue"
      ]
    },
    {
      "name": "4-channel",
      "shortName": "4ch",
      "channels": [
        "Red",
        "Green",
        "Blue",
        "Intensity"
      ]
    },
    {
      "name": "5-channel",
      "shortName": "5ch",
      "channels": [
        "Red",
        "Green",
        "Blue",
        "Intensity",
        "Color Macros"
      ]
    },
    {
      "name": "6-channel",
      "shortName": "6ch",
      "channels": [
        "Red",
        "Green",
        "Blue",
        "Color Macros",
        "Strobe",
        "Intensity"
      ]
    },
    {
      "name": "7-channel",
      "shortName": "7ch",
      "channels": [
        "Red",
        "Green",
        "Blue",
        "Color Macros",
        "Strobe / Program Speed / Sound Sensitivity",
        "Program",
        "Intensity"
      ]
    }
  ]
}
