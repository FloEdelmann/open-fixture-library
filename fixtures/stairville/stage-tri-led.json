{
  "$schema": "https://raw.githubusercontent.com/FloEdelmann/open-fixture-library/master/schema-fixture.json",
  "name": "Stage TRI LED",
  "categories": ["Color Changer"],
  "meta": {
    "authors": ["p_0g_8mm3_"],
    "createDate": "2018-01-30",
    "lastModifyDate": "2018-01-30"
  },
  "manualURL": "https://images.thomann.de/pics/prod/238663_manual.pdf",
  "physical": {
    "dimensions": [1200, 2400, 10],
    "weight": 16,
    "power": 90,
    "DMXconnector": "3-pin",
    "bulb": {
      "type": "4x 7x 3W RGB LEDs"
    },
    "lens": {
      "degreesMinMax": [40, 40]
    },
    "focus": {
      "type": "Fixed"
    }
  },
  "matrix": {
    "pixelKeys": [
      [
        ["1", "2", "3", "4"]
      ]
    ],
    "pixelGroups": {
      "1+2": ["1", "2"],
      "3+4": ["3", "4"],
      "Master": ["1", "2", "3", "4"]
    }
  },
  "availableChannels": {
    "Dimmer": {
      "type": "Intensity"
    },
    "Program": {
      "type": "Effect",
      "capabilities": [
        {
          "range": [0, 9],
          "name": "Off"
        },
        {
          "range": [10, 19],
          "name": "Red"
        },
        {
          "range": [20, 29],
          "name": "Yellow"
        },
        {
          "range": [30, 39],
          "name": "Green"
        },
        {
          "range": [40, 49],
          "name": "Cyan"
        },
        {
          "range": [50, 59],
          "name": "Blue"
        },
        {
          "range": [60, 69],
          "name": "Violet"
        },
        {
          "range": [70, 79],
          "name": "White"
        },
        {
          "range": [80, 89],
          "name": "Program 1"
        },
        {
          "range": [90, 99],
          "name": "Program 2"
        },
        {
          "range": [100, 109],
          "name": "Program 3"
        },
        {
          "range": [110, 119],
          "name": "Program 4"
        },
        {
          "range": [120, 129],
          "name": "Program 5"
        },
        {
          "range": [130, 139],
          "name": "Program 6"
        },
        {
          "range": [140, 149],
          "name": "Program 7"
        },
        {
          "range": [150, 159],
          "name": "Program 8"
        },
        {
          "range": [160, 169],
          "name": "Program 9"
        },
        {
          "range": [170, 179],
          "name": "Program 10"
        },
        {
          "range": [180, 189],
          "name": "Program 11"
        },
        {
          "range": [190, 199],
          "name": "Program 12"
        },
        {
          "range": [200, 209],
          "name": "Program 13"
        },
        {
          "range": [210, 219],
          "name": "Program 14"
        },
        {
          "range": [220, 229],
          "name": "Program 15"
        },
        {
          "range": [230, 239],
          "name": "Program 16"
        },
        {
          "range": [240, 255],
          "name": "Sound control"
        }
      ]
    },
    "Speed / Sound Sensitivity": {
      "type": "Speed"
    },
    "Strobe": {
      "type": "Strobe"
    },
    "Mode": {
      "type": "Effect",
      "capabilities": [
        {
          "range": [0, 9],
          "name": "RGB"
        },
        {
          "range": [10, 249],
          "name": "Programs"
        },
        {
          "range": [250, 255],
          "name": "Sound control"
        }
      ]
    }
  },
  "templateChannels": {
    "Red $pixelKey": {
      "type": "Single Color",
      "color": "Red"
    },
    "Green $pixelKey": {
      "type": "Single Color",
      "color": "Green"
    },
    "Blue $pixelKey": {
      "type": "Single Color",
      "color": "Blue"
    }
  },
  "modes": [
    {
      "name": "RGB",
      "shortName": "d-P1",
      "channels": [
        "Red Master",
        "Green Master",
        "Blue Master"
      ]
    },
    {
      "name": "RGBD",
      "shortName": "d-P2",
      "channels": [
        "Red Master",
        "Green Master",
        "Blue Master",
        "Dimmer"
      ]
    },
    {
      "name": "Color Program",
      "shortName": "d-P7",
      "channels": [
        "Red Master",
        "Green Master",
        "Blue Master",
        "Program",
        "Speed / Sound Sensitivity",
        "Strobe",
        "Dimmer"
      ]
    },
    {
      "name": "Split",
      "shortName": "d-P3",
      "channels": [
        "Red 1+2",
        "Green 1+2",
        "Blue 1+2",
        "Red 3+4",
        "Green 3+4",
        "Blue 3+4",
        "Strobe",
        "Dimmer"
      ]
    },
    {
      "name": "Individual",
      "shortName": "d-P4",
      "channels": [
        {
          "insert": "matrixChannels",
          "repeatFor": "eachPixelABC",
          "channelOrder": "perPixel",
          "templateChannels": [
            "Red $pixelKey",
            "Green $pixelKey",
            "Blue $pixelKey"
          ]
        },
        "Strobe",
        "Dimmer"
      ]
    },
    {
      "name": "Complete",
      "shortName": "d-P6",
      "channels": [
        "Mode",
        "Dimmer",
        "Strobe",
        {
          "insert": "matrixChannels",
          "repeatFor": "eachPixelABC",
          "channelOrder": "perPixel",
          "templateChannels": [
            "Red $pixelKey",
            "Green $pixelKey",
            "Blue $pixelKey"
          ]
        }
      ]
    }
  ]
}