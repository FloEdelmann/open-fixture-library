{
  "$schema": "https://raw.githubusercontent.com/FloEdelmann/open-fixture-library/master/schemas/fixture.json",
  "name": "ALC4",
  "shortName": "alc4",
  "categories": ["Blinder", "Color Changer"],
  "meta": {
    "authors": ["ameisso"],
    "createDate": "2018-02-19",
    "lastModifyDate": "2018-02-19"
  },
  "manualURL": "http://www.adblighting.com/?page=getsupportfile&file=165",
  "physical": {
    "dimensions": [496, 268, 182],
    "weight": 10,
    "power": 200,
    "DMXconnector": "5-pin",
    "bulb": {
      "type": "LED",
      "lumens": 4000
    },
    "focus": {
      "type": "Fixed"
    }
  },
  "matrix": {
    "pixelKeys": [
      [
        ["4", "3", "2", "1"]
      ]
    ]
  },
  "availableChannels": {
    "Dimmer": {
      "type": "Intensity",
      "defaultValue": 0
    },
    "Color Temperature": {
      "type": "Color Temperature",
      "defaultValue": 0
    },
    "Red": {
      "type": "Single Color",
      "color": "Red"
    },
    "Green": {
      "type": "Single Color",
      "color": "Green"
    },
    "Blue": {
      "type": "Single Color",
      "color": "Blue"
    },
    "Master": {
      "type": "Intensity"
    }
  },
  "templateChannels": {
    "Dimmer $pixelKey": {
      "type": "Intensity"
    },
    "Color Temperature $pixelKey": {
      "type": "Color Temperature"
    },
    "Red $pixelKey": {
      "type": "Single Color",
      "color": "Red"
    },
    "Green $pixelKey": {
      "type": "Single Color",
      "color": "Green"
    },
    "Blue $pixelKey": {
      "type": "Single Color",
      "color": "Blue"
    }
  },
  "modes": [
    {
      "name": "Standard",
      "shortName": "standard",
      "channels": [
        "Dimmer",
        "Color Temperature",
        "Red",
        "Green",
        "Blue"
      ]
    },
    {
      "name": "Matrix ",
      "shortName": "matrix",
      "channels": [
        {
          "insert": "matrixChannels",
          "repeatFor": "eachPixelABC",
          "channelOrder": "perPixel",
          "templateChannels": [
            "Dimmer $pixelKey",
            "Color Temperature $pixelKey",
            "Red $pixelKey",
            "Green $pixelKey",
            "Blue $pixelKey"
          ]
        }
      ]
    },
    {
      "name": "Extended",
      "shortName": "extended",
      "channels": [
        "Master",
        {
          "insert": "matrixChannels",
          "repeatFor": "eachPixelABC",
          "channelOrder": "perPixel",
          "templateChannels": [
            "Dimmer $pixelKey",
            "Color Temperature $pixelKey",
            "Red $pixelKey",
            "Green $pixelKey",
            "Blue $pixelKey"
          ]
        }
      ]
    }
  ]
}