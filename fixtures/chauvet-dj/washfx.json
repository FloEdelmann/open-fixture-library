{
  "$schema": "https://raw.githubusercontent.com/OpenLightingProject/open-fixture-library/master/schemas/fixture.json",
  "name": "WashFX",
  "categories": ["Color Changer"],
  "meta": {
    "authors": ["Justin Hornsby", "Felix Edelmann"],
    "createDate": "2017-11-21",
    "lastModifyDate": "2017-12-31"
  },
  "manualURL": "https://www.chauvetdj.com/wp-content/uploads/2015/12/Wash_FX_UM_Rev1_WO-1.pdf",
  "physical": {
    "dimensions": [215, 255, 180],
    "weight": 2.2,
    "power": 75,
    "DMXconnector": "3-pin",
    "bulb": {
      "type": "LED"
    },
    "lens": {
      "degreesMinMax": [90, 90]
    },
    "focus": {
      "type": "Fixed"
    }
  },
  "matrix": {
    "pixelKeys": [
      [
        ["4", "5", "6"],
        ["3", "2", "1"]
      ]
    ]
  },
  "availableChannels": {
    "Red": {
      "type": "Single Color",
      "color": "Red"
    },
    "Green": {
      "type": "Single Color",
      "color": "Green"
    },
    "Blue": {
      "type": "Single Color",
      "color": "Blue"
    },
    "Strobe": {
      "type": "Strobe",
      "capabilities": [
        {
          "range": [0, 255],
          "name": "0-20Hz"
        }
      ]
    },
    "Auto Program": {
      "type": "Effect",
      "capabilities": [
        {
          "range": [0, 5],
          "name": "No function"
        },
        {
          "range": [6, 20],
          "name": "Pattern 1"
        },
        {
          "range": [21, 35],
          "name": "Pattern 2"
        },
        {
          "range": [36, 50],
          "name": "Pattern 3"
        },
        {
          "range": [51, 65],
          "name": "Pattern 4"
        },
        {
          "range": [66, 80],
          "name": "Pattern 5"
        },
        {
          "range": [81, 95],
          "name": "Pattern 6"
        },
        {
          "range": [96, 110],
          "name": "Pattern 7"
        },
        {
          "range": [111, 125],
          "name": "Pattern 8"
        },
        {
          "range": [126, 140],
          "name": "Pattern 9"
        },
        {
          "range": [141, 155],
          "name": "Pattern 10"
        },
        {
          "range": [156, 170],
          "name": "Pattern 11"
        },
        {
          "range": [171, 185],
          "name": "Pattern 12"
        },
        {
          "range": [186, 200],
          "name": "Pattern 13"
        },
        {
          "range": [201, 215],
          "name": "Pattern 14"
        },
        {
          "range": [216, 230],
          "name": "Pattern 15"
        },
        {
          "range": [231, 245],
          "name": "Pattern 16"
        },
        {
          "range": [246, 255],
          "name": "Sound Active"
        }
      ]
    },
    "Auto Program Speed": {
      "type": "Speed"
    },
    "Auto or Sound Program": {
      "type": "Effect",
      "capabilities": [
        {
          "range": [0, 10],
          "name": "No function"
        },
        {
          "range": [11, 127],
          "name": "Auto"
        },
        {
          "range": [128, 255],
          "name": "Sound Active"
        }
      ]
    },
    "Master Dimmer": {
      "type": "Intensity"
    }
  },
  "templateChannels": {
    "Red $pixelKey": {
      "type": "Single Color",
      "color": "Red"
    },
    "Green $pixelKey": {
      "type": "Single Color",
      "color": "Green"
    },
    "Blue $pixelKey": {
      "type": "Single Color",
      "color": "Blue"
    }
  },
  "modes": [
    {
      "name": "7ch",
      "channels": [
        "Red",
        "Green",
        "Blue",
        "Strobe",
        "Auto Program",
        "Auto Program Speed",
        "Auto or Sound Program"
      ]
    },
    {
      "name": "23ch",
      "channels": [
        "Master Dimmer",
        "Auto Program",
        "Auto Program Speed",
        "Strobe",
        "Auto or Sound Program",
        {
          "insert": "matrixChannels",
          "repeatFor": "eachPixelABC",
          "channelOrder": "perPixel",
          "templateChannels": [
            "Red $pixelKey",
            "Green $pixelKey",
            "Blue $pixelKey"
          ]
        }
      ]
    }
  ]
}
