{
  "$schema": "https://raw.githubusercontent.com/OpenLightingProject/open-fixture-library/master/schemas/fixture.json",
  "name": "Geyser RGB",
  "categories": ["Smoke"],
  "meta": {
    "authors": ["Alex Jacobs"],
    "createDate": "2019-10-16",
    "lastModifyDate": "2019-10-16"
  },
  "links": {
    "manual": [
      "https://www.chauvetdj.com/wp-content/uploads/2015/12/GeyserRGB_UM_Rev8_WO-1.pdf"
    ]
  },
  "availableChannels": {
    "Fog": {
      "defaultValue": 0,
      "capabilities": [
        {
          "dmxRange": [0, 9],
          "type": "NoFunction"
        },
        {
          "dmxRange": [10, 255],
          "type": "Fog",
          "fogType": "Fog"
        }
      ]
    },
    "Red": {
      "capabilities": [
        {
          "dmxRange": [0, 9],
          "type": "NoFunction"
        },
        {
          "dmxRange": [10, 255],
          "type": "ColorIntensity",
          "color": "Red"
        }
      ]
    },
    "Green": {
      "capabilities": [
        {
          "dmxRange": [0, 9],
          "type": "NoFunction"
        },
        {
          "dmxRange": [10, 255],
          "type": "ColorIntensity",
          "color": "Green"
        }
      ]
    },
    "Blue": {
      "capabilities": [
        {
          "dmxRange": [0, 9],
          "type": "NoFunction"
        },
        {
          "dmxRange": [10, 255],
          "type": "ColorIntensity",
          "color": "Blue"
        }
      ]
    },
    "Color Mixing": {
      "capabilities": [
        {
          "dmxRange": [0, 9],
          "type": "NoFunction"
        },
        {
          "dmxRange": [10, 255],
          "type": "Generic",
          "comment": "Moves through color presets"
        }
      ]
    },
    "Auto Speed": {
      "capabilities": [
        {
          "dmxRange": [0, 9],
          "type": "NoFunction"
        },
        {
          "dmxRange": [10, 255],
          "type": "Speed",
          "speedStart": "slow",
          "speedEnd": "fast"
        }
      ]
    },
    "Strobe": {
      "capabilities": [
        {
          "dmxRange": [0, 9],
          "type": "NoFunction"
        },
        {
          "dmxRange": [10, 255],
          "type": "StrobeSpeed",
          "speedStart": "slow",
          "speedEnd": "fast"
        }
      ]
    },
    "Dimmer": {
      "capabilities": [
        {
          "dmxRange": [0, 9],
          "type": "NoFunction"
        },
        {
          "dmxRange": [10, 255],
          "type": "Intensity"
        }
      ]
    }
  },
  "modes": [
    {
      "name": "Fog",
      "channels": [
        "Fog",
        "Red",
        "Green",
        "Blue",
        "Color Mixing",
        "Auto Speed",
        "Strobe",
        "Dimmer"
      ]
    }
  ]
}
