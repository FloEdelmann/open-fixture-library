{
  "$schema": "https://raw.githubusercontent.com/OpenLightingProject/open-fixture-library/master/schemas/fixture.json",
  "name": "SlimPAR T12 BT",
  "shortName": "ChauvetSlimPART12BT",
  "categories": ["Color Changer"],
  "meta": {
    "authors": ["Flo Edelmann"],
    "createDate": "2018-03-27",
    "lastModifyDate": "2018-03-27"
  },
  "manualURL": "https://www.chauvetdj.eu/wp-content/uploads/2018/03/SlimPAR_T12_BT_UM_Rev2_WO.pdf",
  "physical": {
    "dimensions": [214, 77, 264],
    "weight": 1.2,
    "power": 42,
    "DMXconnector": "3-pin",
    "bulb": {
      "type": "12x 2.5W RGBA LED",
      "lumens": 1000
    },
    "lens": {
      "degreesMinMax": [14, 27]
    },
    "focus": {
      "type": "Fixed"
    }
  },
  "availableChannels": {
    "Red": {
      "type": "Single Color",
      "color": "Red",
      "crossfade": true
    },
    "Green": {
      "type": "Single Color",
      "color": "Green",
      "crossfade": true
    },
    "Blue": {
      "type": "Single Color",
      "color": "Blue",
      "crossfade": true
    },
    "Strobe": {
      "type": "Strobe",
      "capabilities": [
        {
          "range": [0, 255],
          "name": "0 - 20Hz"
        }
      ]
    },
    "Auto Program": {
      "type": "Effect",
      "capabilities": [
        {
          "range": [0, 5],
          "name": "No function"
        },
        {
          "range": [6, 55],
          "name": "Program 1"
        },
        {
          "range": [56, 106],
          "name": "Program 2"
        },
        {
          "range": [107, 157],
          "name": "Program 3"
        },
        {
          "range": [158, 208],
          "name": "Program 4"
        },
        {
          "range": [209, 255],
          "name": "Program 5"
        }
      ]
    },
    "Auto Program Speed": {
      "type": "Speed",
      "capabilities": [
        {
          "range": [0, 250],
          "name": "slow - fast"
        },
        {
          "range": [251, 255],
          "name": "Sound active mode"
        }
      ]
    },
    "Dimmer": {
      "type": "Intensity",
      "crossfade": true
    }
  },
  "modes": [
    {
      "name": "3-channel",
      "shortName": "3ch",
      "channels": [
        "Red",
        "Green",
        "Blue"
      ]
    },
    {
      "name": "7-channel",
      "shortName": "7ch",
      "channels": [
        "Red",
        "Green",
        "Blue",
        "Strobe",
        "Auto Program",
        "Auto Program Speed",
        "Dimmer"
      ]
    }
  ]
}
