        {
          "range": [227, 255],
          "name": "Sound program",
          "switchChannels": {
            "Strobe / Speed / Sound Sensitivity": "Sound Sensitivity"
          }
        }
      ]
    }
  },
  "templateChannels": {
    "Red $pixelKey": {
      "type": "Single Color",
      "color": "Red",
      "crossfade": true
    },
    "Green $pixelKey": {
      "type": "Single Color",
      "color": "Green",
      "crossfade": true
    },
    "Blue $pixelKey": {
      "type": "Single Color",
      "color": "Blue",
      "crossfade": true
    }
  },
  "modes": [
    {
      "name": "3-channel",
      "shortName": "3ch",
      "channels": [
        "Red All Zones",
        "Green All Zones",
        "Blue All Zones"
      ]
    },
    {
      "name": "6-channel",
      "shortName": "6ch",
      "channels": [
        "Red All Zones",
        "Green All Zones",
        "Blue All Zones",
        "Dimmer",
        "Strobe",
        "Dimmer Speed"
      ]
    },
    {
      "name": "8-channel",
      "shortName": "8ch",
      "channels": [
        "Red All Zones",
        "Green All Zones",
        "Blue All Zones",
