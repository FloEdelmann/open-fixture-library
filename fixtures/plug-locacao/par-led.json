{
  "$schema": "https://raw.githubusercontent.com/OpenLightingProject/open-fixture-library/master/schemas/fixture.json",
  "name": "PAR LED",
  "categories": ["Color Changer"],
  "meta": {
    "authors": ["BIGU L"],
    "createDate": "2018-12-10",
    "lastModifyDate": "2018-12-10"
  },
  "availableChannels": {
    "Intensity": {
      "capability": {
        "type": "Intensity"
      }
    },
    "Red": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Red"
      }
    },
    "Green": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Green"
      }
    },
    "Blue": {
      "capability": {
        "type": "ColorIntensity",
        "color": "Blue"
      }
    },
    "White": {
      "capability": {
        "type": "ColorIntensity",
        "color": "White"
      }
    },
    "Strobe": {
      "capability": {
        "type": "ShutterStrobe",
        "shutterEffect": "Strobe",
        "soundControlled": false,
        "speedStart": "slow",
        "speedEnd": "fast"
      }
    },
    "Color Macros": {
      "capability": {
        "type": "ColorWheelRotation",
        "speed": "fast CW"
      }
    }
  },
  "modes": [
    {
      "name": "7-channel",
      "shortName": "7ch",
      "channels": [
        "Intensity",
        "Red",
        "Green",
        "Blue",
        "White",
        "Strobe",
        "Color Macros"
      ]
    }
  ]
}
