{
  "$schema": "https://raw.githubusercontent.com/OpenLightingProject/open-fixture-library/master/schemas/fixture.json",
  "name": "Ventilator",
  "shortName": "Venti",
  "categories": ["Fan", "Color Changer"],
  "comment": "(C) Görli + Edelnörd 2016",
  "meta": {
    "authors": ["Flo Edelmann"],
    "createDate": "2017-02-24",
    "lastModifyDate": "2017-10-16"
  },
  "physical": {
    "dimensions": [1000, 1000, 160],
    "weight": 10.0,
    "power": 48,
    "DMXconnector": "3-pin",
    "bulb": {
      "type": "LED"
    },
    "focus": {
      "type": "Fixed"
    }
  },
  "availableChannels": {
    "Motor Speed": {
      "type": "Speed",
      "defaultValue": 128,
      "crossfade": true,
      "capabilities": [
        {
          "range": [0, 3],
          "name": "Off"
        },
        {
          "range": [4, 4],
          "name": "CCW full speed"
        },
        {
          "range": [5, 119],
          "name": "CCW",
          "menuClick": "center"
        },
        {
          "range": [120, 139],
          "name": "Off",
          "menuClick": "center"
        },
        {
          "range": [140, 254],
          "name": "CW",
          "menuClick": "center"
        },
        {
          "range": [255, 255],
          "name": "CW full speed"
        }
      ]
    },
    "Red": {
      "type": "Single Color",
      "color": "Red",
      "highlightValue": 255,
      "crossfade": true,
      "precedence": "HTP"
    },
    "Green": {
      "type": "Single Color",
      "color": "Green",
      "highlightValue": 255,
      "crossfade": true,
      "precedence": "HTP"
    },
    "Blue": {
      "type": "Single Color",
      "color": "Blue",
      "highlightValue": 255,
      "crossfade": true,
      "precedence": "HTP"
    },
    "Strobe": {
      "type": "Strobe",
      "highlightValue": 230
    }
  },
  "modes": [
    {
      "name": "5-channel",
      "shortName": "5ch",
      "channels": ["Motor Speed", "Red", "Green", "Blue", "Strobe"]
    }
  ]
}
